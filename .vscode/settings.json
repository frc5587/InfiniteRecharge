{
  "java.configuration.updateBuildConfiguration": "automatic",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "code-runner.customCommand": "cd \"$workspaceRoot\" && ./gradlew build",
  "code-runner.executorMap": {
    "java": "cd \"$workspaceRoot\" && ./gradlew deploy"
  }
}